<!DOCTYPE html>


<html lang="en" style="padding-bottom: 30px !important;">
<head>
    <meta charset="utf-8" />
    <title>@LC.CORE.Helpers.ConstantHelpers.PROJECT | Dashboard</title>
    <meta name="description" content="@LC.CORE.Helpers.ConstantHelpers.PROJECT">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no">
    <link rel="shortcut icon" href="~/images/logo/favicon.ico" />
    <!--begin::Web font -->
    <script src="https://ajax.googleapis.com/ajax/libs/webfont/1.6.16/webfont.js"></script>
    <script>
        WebFont.load({
            google: { "families": ["Poppins:300,400,500,600,700", "Roboto:300,400,500,600,700"] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>

     <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=GTM-PDM45VG">
    </script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', 'GTM-PDM45VG');
    </script>

    <!-- Google Tag Manager -->
    <script>
        (function (w, d, s, l, i) {
            w[l] = w[l] || []; w[l].push({
                'gtm.start':
                    new Date().getTime(), event: 'gtm.js'
            }); var f = d.getElementsByTagName(s)[0],
                j = d.createElement(s), dl = l != 'dataLayer' ? '&l=' + l : ''; j.async = true; j.src =
                    'https://www.googletagmanager.com/gtm.js?id=' + i + dl; f.parentNode.insertBefore(j, f);
        })(window, document, 'script', 'dataLayer', 'GTM-PDM45VG');</script>
    <!-- End Google Tag Manager -->
    <!-- Google Tag Manager (noscript) -->
    <noscript>
        <iframe src="https://www.googletagmanager.com/ns.html?id=GTM-PDM45VG"
                height="0" width="0" style="display:none;visibility:hidden"></iframe>
    </noscript>
    <!-- End Google Tag Manager (noscript) -->

    <link href="~/assets/css/vendors.client.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/css/style.client.bundle.css" rel="stylesheet" type="text/css" />
    <link href="~/assets/plugins/dataTable/jquery.dataTables.min.css" rel="stylesheet" />
    <link href="~/assets/css/styles.css" rel="stylesheet" type="text/css" />
    <link href="~/css/site.css" asp-append-version="true" rel="stylesheet" type="text/css" />
    <link href="~/css/site_client.css" asp-append-version="true" rel="stylesheet" type="text/css" />
    @RenderSection("Styles", required: false)
</head>

<!-- end::Head -->
<!-- begin::Body -->
<body class="m-page--wide  m-page--loading-enabled m-page--loading m-header--fixed m-header--fixed-mobile m-footer--push m-aside--offcanvas-default" style="background-color:#f2f6f9;">
    <!-- begin:: Page -->
    <div class="m-grid m-grid--hor m-grid--root m-page" style="position:relative;">
        <!-- begin::Header -->

        <header id="m_header" class="m-grid__item    m-header lc-layout-header" m-minimize-offset="200" m-minimize-mobile-offset="200">
            <div class="m-container m-container--responsive m-container--xxl m-container--full-height m-page__container">
                <div class="m-stack m-stack--ver m-stack--desktop">

                    <!-- BEGIN: Brand -->
                    <div class="m-stack__item m-brand  m-brand--skin-dark ">
                        <div class="m-stack m-stack--ver m-stack--general">
                            <div class="m-stack__item m-stack__item--middle m-brand__logo">
                                <a href="/" class="m-brand__logo-wrapper">
                                    <img class="img_logo" alt="Legal Connection" src="~/assets/images/logo_horizontal_blanco.png">
                                </a>
                            </div>
                            <div class="m-stack__item m-stack__item--middle m-brand__tools">

                                <!-- BEGIN: Responsive Header Menu Toggler -->
                                <a id="m_aside_header_menu_mobile_toggle" href="javascript:;" class="m-brand__icon m-brand__toggler m--visible-tablet-and-mobile-inline-block">
                                    <span></span>
                                </a>

                                <!-- END -->
                                <!-- BEGIN: Topbar Toggler -->


                                <a id="m_aside_header_topbar_mobile_toggle" href="javascript:;" class="m-brand__icon m--visible-tablet-and-mobile-inline-block">
                                    <i class="flaticon-more"></i>
                                </a>

                                <!-- BEGIN: Topbar Toggler -->
                            </div>
                        </div>
                    </div>

                    <!-- END: Brand -->
                    <div class="m-stack__item m-stack__item--fluid m-header-head" id="m_header_nav" style="height:10px !important;">

                        <!-- BEGIN: Horizontal Menu -->
                        <button class="m-aside-header-menu-mobile-close  m-aside-header-menu-mobile-close--skin-dark " id="m_aside_header_menu_mobile_close_btn"><i class="la la-close"></i></button>
                        <div id="m_header_menu" class="m-header-menu m-aside-header-menu-mobile m-aside-header-menu-mobile--offcanvas m-header-menu--skin-dark m-header-menu--submenu-skin-light m-aside-header-menu-mobile--skin-dark m-aside-header-menu-mobile--submenu-skin-dark float-left">
                            <ul class="m-menu__nav ">
                                @await Html.PartialAsync("Partials/_TopBarMenuPartial")
                            </ul>
                        </div>

                        <!-- END: Horizontal Menu -->
                        <!-- BEGIN: Topbar -->
                        @await Html.PartialAsync("Partials/_TopBarUserPartial")
                        <!-- END: Topbar -->
                    </div>
                </div>
            </div>
        </header>


        <!-- end::Header -->
        <!-- begin::Body -->
        <div class="m-grid__item m-grid__item--fluid  m-grid m-grid--ver-desktop m-grid--desktop 	m-container m-container--fluid m-container--responsive m-container--xxl m-page__container m-body">
            <div class="m-grid__item m-grid__item--fluid m-wrapper">

                <!-- END: Subheader -->
                <div class="m-content pt-0">
                    @RenderBody()
                </div>
            </div>
        </div>

        <!-- end::Body -->
        <!-- begin::Footer -->
        <footer class="m-grid__item		m-footer lc-footer-2">
            <div class="m-container m-container--fluid m-container--full-height m-page__container">
                @await Html.PartialAsync("Partials/_FooterPartial")
            </div>
        </footer>
        <!-- end::Footer -->
    </div>

    <!-- end:: Page -->
    <!-- end::Quick Sidebar -->
    <!-- begin::Scroll Top -->
    <div id="m_scroll_top" class="m-scroll-top">
        <i class="la la-arrow-up"></i>
    </div>
    <div id="loading"><img src="~/assets/images/Pulse-1.8s-200px.gif" /></div>
    <!-- end::Scroll Top -->
    <!-- begin::Quick Nav -->
    <script src="~/assets/plugins/jquery.js" type="text/javascript"></script>
    <script src="~/assets/js/vendors.bundle.js" type="text/javascript"></script>
    <script src="~/assets/js/scripts.bundle.js" type="text/javascript"></script>
    <script src="~/assets/plugins/dataTable/jquery.dataTables.min.js"></script>
    <script src="~/assets/plugins/select2/select2.min.js"></script>
    @*<script src="~/assets/plugins/select2/select2.full.min.js"></script>*@
    <script src="~/assets/js/signalr/signalr.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/views/shared/topbar.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)

    <script>
        $(window).on('load', function () { $('body').removeClass('m-page--loading'); });
    </script>

</body>

</html>